<div class="content">
  <h1>Our Products</h1>
  
  <div class="container-fluid">
    <div class="row">
      <% @products.each do |product| %>
        <% logger.debug %>
        <%= render product %>
      <% end %>
    </div>
  </div>
  <br>
  <div class="col-sm-12">
    <%= link_to 'New Product', new_product_path, class: "btn btn-forms" %>
  </div>
</div>